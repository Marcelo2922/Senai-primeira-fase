programa {
  funcao inteiro soma (inteiro valor1, inteiro valor2){
    retorne valor1 + valor2

  }
  funcao inteiro muti (inteiro valor1, inteiro valor2){
    retorne valor1 * valor2
  }
  funcao inteiro subi (inteiro valor1, inteiro valor2){
    retorne valor1 - valor2
  }
  funcao inicio() {
    escreva (soma (5,3),"\n")
    escreva (muti(10,3), "\n")
    escreva (subi(12,6), "\n")
    
  }
}
